            {
              "id": "func-cl-config-mgmt",
              "title": "配置管理",
              "description": "管理和配置系统的基本设置，包括标题维护、logo配置等。",
              "version": "1.0", "status": "已上线", "tags": ["配置"], "documentLink": null
            }
          ]
        }
      ]
