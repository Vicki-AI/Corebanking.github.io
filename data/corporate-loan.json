            {
              "id": "func-cl-postloan-mgmt",
              "title": "贷后管理",
              "description": "对公贷款借据的贷后管理，包括：利息调整、核销管理等功能。",
              "version": "1.0", "status": "已上线", "tags": ["贷后"], "documentLink": null
