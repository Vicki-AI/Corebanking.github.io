            },
            {
              "id": "func-er-limit-status",
              "title": "额度状态管理",
              "description": "可查询额度信息，对额度进行冻结解冻终止操作，并对冻结解冻终止进行复核审批。",
              "version": "1.0", "status": "已上线", "tags": ["额度", "状态管理"], "documentLink": null
            },
            {
              "id": "func-er-limit-config",
              "title": "配置管理",
              "description": "授信产品和批量配置管理。",
              "version": "1.0", "status": "已上线", "tags": ["额度", "配置"], "documentLink": null
            }
          ]
        },
        {
          "name": "风险名单管理",
          "functions": [
            {
              "id": "func-er-list-mgmt",
